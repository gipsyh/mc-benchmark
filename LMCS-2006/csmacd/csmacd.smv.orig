MODULE main
VAR
	r_medium : process ac_Medium(r_medium, r_mac1, r_mac2);
	r_mac1 : process ac_Mac(r_mac1, r_medium, TRUE);
	r_mac2 : process ac_Mac(r_mac2, r_medium, FALSE);

LTLSPEC
	--(G((!r_mac1.s_receivedSend)->F(s_receivedSend)))->
	G ((!r_medium.queueOverFlow)&(!r_mac1.queueOverFlow)&(!r_mac2.queueOverFlow));
LTLSPEC
	G ((r_mac1.s_hasSent)->F(r_mac2.s_hasReceived));
LTLSPEC
	G ((r_mac2.s_hasSent)->F(r_mac1.s_hasReceived));

