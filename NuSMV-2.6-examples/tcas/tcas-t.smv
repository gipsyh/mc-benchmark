DEFINE
	t273a :=  Corrective_Descend_Evaluated_Event
		& Composite_RA = No_RA
		& !Some_OA_Altitude_Reporting_Lost
		& !Some_Threat_Track_Dropped
		& !Some_Threat_Clear_Of_Conflict;  

	t273b :=  Corrective_Descend_Evaluated_Event
		& in_RA
		& Corrective_Climb = Yes;	  

	t273c :=  Corrective_Descend_Evaluated_Event
		& in_RA
		& Corrective_Descend = Yes;	  

	t273d :=  Corrective_Descend_Evaluated_Event
		& in_RA
		& Corrective_Climb = No	  
		& Corrective_Descend = No;
		
	t273e :=  Corrective_Descend_Evaluated_Event
		& Composite_RA = No_RA
		& Some_OA_Altitude_Reporting_Lost		
		& (  Combined_Control = Preventitive
		   | Combined_Control = CC_Corrective_Climb
		   | Combined_Control = CC_Corrective_Descend );

	t273f :=  Corrective_Descend_Evaluated_Event
		& Composite_RA = No_RA
		& !Some_OA_Altitude_Reporting_Lost		
		& Some_Threat_Track_Dropped
		& (  Combined_Control = Preventitive
		   | Combined_Control = CC_Corrective_Climb
		   | Combined_Control = CC_Corrective_Descend );

	t273g :=  Corrective_Descend_Evaluated_Event
		& Composite_RA = No_RA
		& !Some_OA_Altitude_Reporting_Lost		
		& !Some_Threat_Track_Dropped
		& Some_Threat_Clear_Of_Conflict  
		& (  Combined_Control = Preventitive
		   | Combined_Control = CC_Corrective_Climb
		   | Combined_Control = CC_Corrective_Descend );

	ND_Combined_Control :=
		  (t273a & t273b)
		| (t273a & t273c)
		| (t273a & t273d)
		| (t273a & t273e)
		| (t273a & t273f)
		| (t273a & t273g)
		| (t273b & t273d)
		| (t273b & t273e)
		| (t273b & t273f)
		| (t273b & t273g)
		| (t273c & t273d)
		| (t273c & t273e)
		| (t273c & t273f)
		| (t273c & t273g)
		| (t273d & t273e)
		| (t273d & t273f)
		| (t273d & t273g)
		| (t273e & t273f)
		| (t273e & t273g)
		| (t273f & t273g)
		;

ASSIGN

	next(Combined_Control) :=
		case
			t273a : No_Advisory;
			t273b & t273c : {CC_Corrective_Climb, CC_Corrective_Descend};
			t273b : CC_Corrective_Climb;
			t273c : CC_Corrective_Descend;
			t273d : Preventitive;
			t273e : Altitude_Lost;
			t273f : Tracked_Dropped;
			t273g : Clear_Of_Conflict;
			TRUE     : Combined_Control;
		esac;
ASSIGN
	next(Combined_Control_Evaluated_Event) := Corrective_Descend_Evaluated_Event;
			


DEFINE

	t277a :=   Corrective_Descend_Evaluated_Event
