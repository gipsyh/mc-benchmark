		&  !Some_OA_Sense_Descend_Increase
		&  !(Corrective_Climb = Yes)
		&  !Some_OA_Threat_Crossing_Own_Crossing 
		&  !Some_OA_Threat_Crossing_Int_Crossing;

	t277i :=   Corrective_Descend_Evaluated_Event
		&  !Some_OA_Threat_Reversal_Reversed
		&  Composite_RA = Descend
		&  !Some_OA_Sense_Descend_Increase
		&  !(Corrective_Descend = Yes)
		&  !Some_OA_Threat_Crossing_Own_Crossing 
		&  !Some_OA_Threat_Crossing_Int_Crossing;

	t277j :=   Corrective_Descend_Evaluated_Event
		&  Composite_RA = Climb
		&  !Some_OA_Sense_Climb_Increase
		&  !(Corrective_Climb = Yes)
		&  !(Corrective_Descend = Yes);

	t277k :=   Corrective_Descend_Evaluated_Event
		&  Composite_RA = Descend
		&  !Some_OA_Sense_Descend_Increase
		&  !(Corrective_Climb = Yes)
		&  !(Corrective_Descend = Yes);

	t277l :=   Corrective_Descend_Evaluated_Event
		&  Some_OA_Threat_Reversal_Reversed
		&  !(Composite_RA = Climb)
		&  !(Composite_RA = Descend);

	t277m :=   Corrective_Descend_Evaluated_Event
		&  Some_OA_Threat_Reversal_Reversed
		&  Corrective_Climb = Yes;

	t277n :=   Corrective_Descend_Evaluated_Event
		&  Some_OA_Threat_Reversal_Reversed
		&  Corrective_Descend = Yes;	

	t277-A := t277a | t277b | t277c;
	t277-B := t277d | t277e;
	t277-C := t277f | t277g | t277h | t277i;
	t277-D := t277j | t277k;
	t277-E := t277l | t277m;


	ND_Vertical_Control :=
		  (t277-A & t277-B)
		| (t277-A & t277-C)
		| (t277-A & t277-D)
		| (t277-A & t277-E)
		| (t277-B & t277-C)
		| (t277-B & t277-D)
		| (t277-B & t277-E)
		| (t277-C & t277-D)
		| (t277-C & t277-E)
		| (t277-D & t277-E)
		;

ASSIGN

	next(Vertical_Control) :=
		case	
			t277-A : Other;
			t277-B : Increase;
			t277-C : Crossing;
			t277-D : Maintain;
			t277-E : Reversal;
			TRUE : Vertical_Control;
		esac;
ASSIGN
	next(Vertical_Control_Evaluated_Event) := Corrective_Descend_Evaluated_Event;



