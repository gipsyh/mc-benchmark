VAR prev_Advisory : {RA, TA_RA_Delay};
ASSIGN
  next(prev_Advisory) := case
    next(Surveillance_Complete_Event): Advisory;
    TRUE: prev_Advisory;
  esac;


VAR prev_Track_Status : {Threat_Not_Tracked, Not_Tracked, Tracked};
ASSIGN
  next(prev_Track_Status) := case
    next(Surveillance_Complete_Event): Track_Status;
    TRUE: prev_Track_Status;
  esac;

VAR prev_Sense : {Climb,Descend};
ASSIGN
  next(prev_Sense) := case
    next(Surveillance_Complete_Event): Sense;
    TRUE: prev_Sense;
  esac;

VAR prev_Strength : {Strength_Not_Selected, VSL2000, VSL1000, VSL500, VSL0, Nominal, Increase};
ASSIGN
  next(prev_Strength) := case
    next(Surveillance_Complete_Event): Strength;
    TRUE: prev_Strength;
  esac;



ASSIGN

init(Track_Status) := Not_Tracked;
init(prev_Track_Status) := Not_Tracked;

next(Track_Status) := case
	Effective_SL_Evaluated_Event : {Threat_Not_Tracked, Not_Tracked, Tracked};
	TRUE : Track_Status;
	esac;

DEFINE
  Dont_Increase_Climb := 
  	FALSE
	| !in_Sense_Climb_Positive
	| Increase_Climb_Inhibit = Inhibited
	| ((Own_Tracked_Alt_Rate_8=FALSE & (case Own_Tracked_Alt_Rate_8&!FALSE:TRUE; !Own_Tracked_Alt_Rate_8&FALSE:FALSE;
Own_Tracked_Alt_Rate_7&!FALSE:TRUE; !Own_Tracked_Alt_Rate_7&FALSE:FALSE;
Own_Tracked_Alt_Rate_6&!FALSE:TRUE; !Own_Tracked_Alt_Rate_6&FALSE:FALSE;
Own_Tracked_Alt_Rate_5&!TRUE:TRUE; !Own_Tracked_Alt_Rate_5&TRUE:FALSE;
Own_Tracked_Alt_Rate_4&!TRUE:TRUE; !Own_Tracked_Alt_Rate_4&TRUE:FALSE;
Own_Tracked_Alt_Rate_3&!FALSE:TRUE; !Own_Tracked_Alt_Rate_3&FALSE:FALSE;
Own_Tracked_Alt_Rate_2&!FALSE:TRUE; !Own_Tracked_Alt_Rate_2&FALSE:FALSE;
Own_Tracked_Alt_Rate_1&!TRUE:TRUE; !Own_Tracked_Alt_Rate_1&TRUE:FALSE;
 TRUE:TRUE; esac)) | (!Own_Tracked_Alt_Rate_8&FALSE))
	| Climb_Inhibit = Inhibited			
	| No_Positive
	| No_Climb
	;
  Dont_Increase_Descend := 
  	FALSE
	| !in_Sense_Descend_Positive
	| Increase_Descend_Inhibit = Inhibited
	| ((TRUE=Own_Tracked_Alt_Rate_8 & (case TRUE&!Own_Tracked_Alt_Rate_8:TRUE; !TRUE&Own_Tracked_Alt_Rate_8:FALSE;
TRUE&!Own_Tracked_Alt_Rate_7:TRUE; !TRUE&Own_Tracked_Alt_Rate_7:FALSE;
TRUE&!Own_Tracked_Alt_Rate_6:TRUE; !TRUE&Own_Tracked_Alt_Rate_6:FALSE;
FALSE&!Own_Tracked_Alt_Rate_5:TRUE; !FALSE&Own_Tracked_Alt_Rate_5:FALSE;
FALSE&!Own_Tracked_Alt_Rate_4:TRUE; !FALSE&Own_Tracked_Alt_Rate_4:FALSE;
TRUE&!Own_Tracked_Alt_Rate_3:TRUE; !TRUE&Own_Tracked_Alt_Rate_3:FALSE;
TRUE&!Own_Tracked_Alt_Rate_2:TRUE; !TRUE&Own_Tracked_Alt_Rate_2:FALSE;
TRUE&!Own_Tracked_Alt_Rate_1:TRUE; !TRUE&Own_Tracked_Alt_Rate_1:FALSE;
 TRUE:TRUE; esac)) | (!TRUE&Own_Tracked_Alt_Rate_8))
	| Descend_Inhibit = Inhibited			
	| No_Positive
	| No_Descend
	;
  Dont_Descend :=
  	Descend_Inhibit = Inhibited
	| No_Positive
	| No_Descend
	;
  Dont_Climb :=
  	Climb_Inhibit = Inhibited
	| No_Positive
	| No_Climb
	;

ASSIGN

next(Sense) := case
	!Effective_SL_Evaluated_Event : Sense;
	TRUE: {Climb,Descend};
	esac;

DEFINE

SS := case
	Dont_Climb & Dont_Descend & Dont_Increase_Climb & Dont_Increase_Descend:
		{Strength_Not_Selected};
	!Dont_Climb & Dont_Descend & Dont_Increase_Climb & Dont_Increase_Descend: case
		next(Sense) = Climb: {Strength_Not_Selected, Nominal};
		TRUE: {Strength_Not_Selected};
		esac;
	Dont_Climb & !Dont_Descend & Dont_Increase_Climb & Dont_Increase_Descend: case
		next(Sense) = Descend: {Strength_Not_Selected, Nominal};
		TRUE: {Strength_Not_Selected};
		esac;
	Dont_Climb & Dont_Descend & !Dont_Increase_Climb & Dont_Increase_Descend: case
		next(Sense) = Descend: {Strength_Not_Selected};
		TRUE: {Strength_Not_Selected, Increase};
		esac;
	Dont_Climb & Dont_Descend & Dont_Increase_Climb & !Dont_Increase_Descend: case
		next(Sense) = Climb: {Strength_Not_Selected};
		TRUE: {Strength_Not_Selected, Increase};
		esac;
	!Dont_Climb & !Dont_Descend & Dont_Increase_Climb & Dont_Increase_Descend:
		{Strength_Not_Selected, Nominal};
	!Dont_Climb & Dont_Descend & !Dont_Increase_Climb & Dont_Increase_Descend: case
		next(Sense) = Descend: {Strength_Not_Selected};
		TRUE: {Strength_Not_Selected, Nominal, Increase};
		esac;
	!Dont_Climb & Dont_Descend & Dont_Increase_Climb & !Dont_Increase_Descend: case
		next(Sense) = Descend: {Strength_Not_Selected, Increase};
		TRUE: {Strength_Not_Selected, Nominal};
		esac;
	Dont_Climb & !Dont_Descend & !Dont_Increase_Climb & Dont_Increase_Descend: case
		next(Sense) = Climb: {Strength_Not_Selected, Increase};
		TRUE: {Strength_Not_Selected, Nominal};
		esac;
	Dont_Climb & !Dont_Descend & Dont_Increase_Climb & !Dont_Increase_Descend: case
		next(Sense) = Climb: {Strength_Not_Selected};
		TRUE: {Strength_Not_Selected, Nominal, Increase};
		esac;
	Dont_Climb & Dont_Descend & !Dont_Increase_Climb & !Dont_Increase_Descend:
		{Strength_Not_Selected, Increase};
	Dont_Climb & !Dont_Descend & !Dont_Increase_Climb & !Dont_Increase_Descend: case
		next(Sense) = Descend: {Strength_Not_Selected, Nominal, Increase};
		TRUE: {Strength_Not_Selected, Increase};
		esac;
	!Dont_Climb & Dont_Descend & !Dont_Increase_Climb & !Dont_Increase_Descend: case
		next(Sense) = Climb: {Strength_Not_Selected, Nominal, Increase};
		TRUE: {Strength_Not_Selected, Increase};
		esac;
	!Dont_Climb & !Dont_Descend & Dont_Increase_Climb & !Dont_Increase_Descend: case
		next(Sense) = Descend: {Strength_Not_Selected, Nominal, Increase};
		TRUE: {Strength_Not_Selected, Nominal};
		esac;
	!Dont_Climb & !Dont_Descend & !Dont_Increase_Climb & Dont_Increase_Descend: case
		next(Sense) = Climb: {Strength_Not_Selected, Nominal, Increase};
		TRUE: {Strength_Not_Selected, Nominal};
		esac;
	TRUE: {Strength_Not_Selected, Nominal, Increase};
	esac;

ASSIGN
next(Strength) := case
	!Effective_SL_Evaluated_Event: Strength;
	No_Negative: SS;
	TRUE: SS union {VSL2000, VSL1000, VSL500, VSL0};
	esac;



next(Altitude_Reporting) := case
	Effective_SL_Evaluated_Event : {Yes, No, Lost};
	TRUE : Altitude_Reporting;
	esac;
next(Threat_Reversal) := case
	Effective_SL_Evaluated_Event : case
		next(Sense) = Sense: Not_Reversed;
		TRUE: Reversed;
		esac;
	TRUE : Threat_Reversal;
	esac;
next(Threat_Crossing) := case
	Effective_SL_Evaluated_Event : {Int_Crossing, Own_Crossing, No_Crossing};
	TRUE : Threat_Crossing;
	esac;

next(Advisory) := case
	Effective_SL_Evaluated_Event : {RA, TA_RA_Delay};
	TRUE : Advisory;
	esac;



DEFINE

in_Tracked := Track_Status = Tracked;
in_Not_Tracked := Track_Status = Not_Tracked;
in_Sense_Climb := in_Tracked & Sense = Climb;
in_Sense_Descend := in_Tracked & Sense = Descend;
in_Sense_Climb_Positive := in_Sense_Climb & Strength in {Nominal, Increase};
in_Sense_Climb_Negative := in_Sense_Climb & Strength in {VSL2000, VSL1000, VSL500, VSL0};
in_Sense_Descend_Positive := in_Sense_Descend & Strength in {Nominal, Increase};
in_Sense_Descend_Negative := in_Sense_Descend & Strength in {VSL2000, VSL1000, VSL500, VSL0};
in_Sense_Positive := in_Sense_Climb_Positive | in_Sense_Descend_Positive;
in_Sense_Negative := in_Sense_Climb_Negative | in_Sense_Descend_Negative;
in_Sense_Climb_Positive_Increase := in_Sense_Climb & Strength = Increase;
in_Sense_Climb_Positive_Nominal := in_Sense_Climb & Strength = Nominal;
in_Sense_Descend_Positive_Increase := in_Sense_Descend & Strength = Increase;
in_Sense_Descend_Positive_Nominal := in_Sense_Descend & Strength = Nominal;
in_Sense_Climb_Negative_VSL0 := in_Sense_Climb & Strength = VSL0;
in_Sense_Climb_Negative_VSL500 := in_Sense_Climb & Strength = VSL500;
in_Sense_Climb_Negative_VSL1000 := in_Sense_Climb & Strength = VSL1000;
in_Sense_Climb_Negative_VSL2000 := in_Sense_Climb & Strength = VSL2000;
in_Sense_Descend_Negative_VSL0 := in_Sense_Descend & Strength = VSL0;
in_Sense_Descend_Negative_VSL500 := in_Sense_Descend & Strength = VSL500;
in_Sense_Descend_Negative_VSL1000 := in_Sense_Descend & Strength = VSL1000;
in_Sense_Descend_Negative_VSL2000 := in_Sense_Descend & Strength = VSL2000;

in_Altitude_Reporting_Lost := in_Tracked & Altitude_Reporting = Lost;

in_Advisory_RA := in_Tracked & Advisory = RA;

prev_in_Tracked := prev_Track_Status = Tracked;
prev_in_Not_Tracked := prev_Track_Status = Not_Tracked;
prev_in_Sense_Climb := prev_in_Tracked & prev_Sense = Climb;
prev_in_Sense_Descend := prev_in_Tracked & prev_Sense = Descend;
prev_in_Sense_Climb_Positive := prev_in_Sense_Climb & prev_Strength in {Nominal, Increase};
prev_in_Sense_Climb_Negative := prev_in_Sense_Climb & prev_Strength in {VSL2000, VSL1000, VSL500, VSL0};
prev_in_Sense_Descend_Positive := prev_in_Sense_Climb & prev_Strength in {Nominal, Increase};
prev_in_Sense_Descend_Negative := prev_in_Sense_Climb & prev_Strength in {VSL2000, VSL1000, VSL500, VSL0};
prev_in_Sense_Climb_Positive_Increase := prev_in_Sense_Climb & prev_Strength = Increase;
prev_in_Sense_Descend_Positive_Increase := prev_in_Sense_Descend & prev_Strength = Increase;	


prev_in_Advisory_RA := prev_in_Sense_Climb & prev_Advisory = RA;

