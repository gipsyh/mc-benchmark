		&  !Some_OA_Sense_Descend_Increase
		&  !Some_OA_Sense_Climb_Increase
		&  !Some_OA_Threat_Reversal_Reversed
		&  !(Corrective_Descend = Yes)
		&  !Some_OA_Threat_Crossing_Own_Crossing 
